<Window x:Class="_2DRobot_InverseGradient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2DRobot_InverseGradient"
        mc:Ignorable="d"
        Title="2D Robot Fwd+Inverse" Height="320" Width="380">
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions >
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Grid.Column="0">
            <local:DrawingVisualClass x:Name="g" HorizontalAlignment="Left" Width="290" Background="Black" Height="260" VerticalAlignment="Top" MouseLeftButtonUp="g_MouseLeftButtonUp"/>
        </Grid>


    </Grid>
</Window>
